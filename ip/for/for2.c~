#include <stdio.h>
#include <stdlib.h>

int main (){

int soma = 0;

for (int i=5; i <=15; i++){
int d =0;

for (int j=1; j <=15; j++){
if (i%j == 0){
	d++;
}}
printf ("%d\n",d);

if ((d <= 2) && (i%2 == 0)){
soma = soma +(i*3);
} else {
if (i%2 == 0){
soma = soma +(i*1);
} else {
if ((d <= 2) && (i%2 != 0)){
soma = soma +(i*3);
} else {
if (i%2 != 0){
soma = soma +(i*2);
}
}
}
}
}
printf ("%d", soma);
return 0;
}
